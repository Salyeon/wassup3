def plot_mnist_images(images, labels, rows, cols):
    fig, axes = plt.subplots(rows, cols, figsize=(5, 5))
    axes = axes.ravel()
    
    for i in range(rows * cols):
        axes[i].imshow(images[i], cmap='gray')
        axes[i].set_title(labels[i])
        axes[i].axis('off')
        
    plt.tight_layout()
    plt.show()



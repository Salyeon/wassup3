


class UpgradeCalculator(Calculator):
    def __init__(self):
        self.value = 0
    def add(self, val):
        self.value += val
    def minus(self, val):
        self.value -= val


cal = UpgradeCalculator()
cal.add(10)
cal.minus(7)
print(cal.value)





class Calculator:
    def __init__(self):
        self.value = 0
    def add(self, val):
        self.value += val


class MaxLimitCalculator(Calculator):
    def __init__(self):
        self.value = 0
    def add(self, val):
        self.value += val
        if self.value >= 100:
            self.value = 100


cal = MaxLimitCalculator()
cal.add(50)
cal.add(60)

print(cal.value) 





